{
  "name": "tanstack-portfolio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "setup-kv": "echo 'Run: wrangler kv:namespace create CONTENT_CACHE --env production' && echo 'Then update wrangler.toml with the returned ID'",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:staged": "prettier --write --ignore-unknown",
    "pre-push": "npm run type-check && npm run lint && npm run format:check && npm run build",
    "deploy-branch": "npm run build && echo 'Branch built and KV cache updated. Push to GitHub to deploy.'",
    "dev:web": "vite",
    "dev:functions": "wrangler pages dev . --port 8788 --r2 R2_CONTENT=tanstack-portfolio-r2 --kv CONTENT_CACHE --binding ALLOWED_DIRS=blog,portfolio,projects --binding MAX_FILE_BYTES=1048576",
    "dev:functions:local-kv": "wrangler pages dev . --port 8788 --kv CONTENT_CACHE --binding ALLOWED_DIRS=blog,portfolio,projects --binding MAX_FILE_BYTES=1048576",
    "dev:full": "concurrently -n WEB,API -c green,blue \"npm:dev:web\" \"npm:dev:functions\"",
    "deploy:pages": "npm run build && wrangler pages deploy dist --project-name tanstack-portfolio",
    "deploy:worker:proxy": "wrangler publish -c wrangler/wrangler-r2-proxy.toml --env production",
    "deploy:worker:cache-rebuild": "wrangler deploy -c wrangler/wrangler-cache-rebuild.toml --env production",
    "deploy:worker:cache-rebuild-dev": "wrangler deploy -c wrangler/wrangler-cache-rebuild.toml --env development",
    "deploy:worker:kv-get": "wrangler deploy -c wrangler/wrangler-kv-cache-get.toml --env production",
    "deploy:worker:kv-get-dev": "wrangler deploy -c wrangler/wrangler-kv-cache-get.toml --env development"
  },
