{
  "name": "github-file-manager",
  "version": "1.0.0",
  "description": "Cloudflare Worker for GitHub file management",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "deploy:prod": "wrangler deploy --env production",
    "deploy:dev": "wrangler deploy --env development",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/**/*.ts"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241218.0",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "wrangler": "^3.0.0"
  },
  "dependencies": {
    "unified": "^11.0.4",
    "remark": "^15.0.1",
    "remark-parse": "^11.0.0",
    "remark-html": "^16.0.1",
    "remark-gfm": "^4.0.0"
  },
  "keywords": [
    "cloudflare",
    "worker",
    "github",
    "api",
    "file-management"
  ],
  "author": "Roger Lee Cormier",
  "license": "MIT"
}
